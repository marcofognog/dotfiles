filetype plugin indent on
set autoindent
syntax on
set ruler
set hidden
set backspace=indent,eol,start
set shiftwidth=2
set smarttab
set expandtab
set number
set nocompatible
set wildmenu
set hlsearch
set showcmd
set ignorecase
set smartcase
set confirm
set visualbell
set t_vb=
set mouse=a
set cmdheight=2

